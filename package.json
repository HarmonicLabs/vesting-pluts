{
  "name": "vesting-pluts",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf ./dist && tsc -p ./tsconfig.json && tsc-alias -p ./tsconfig.json",
    "build:light": "tsc -p ./tsconfig.json && tsc-alias -p ./tsconfig.json",
    "start": "npm run build && node dist/index.js",
    "vesting:compile": "npm run start",
    "vesting:setup": "npm run build:light && node dist/app/setup.js",
    "vesting:genKeys": "npm run build:light && node dist/app/genKeys.js",
    "vesting:returnFaucet": "npm run build:light && node dist/app/returnFaucet.js",
    "vesting:create": "npm run build:light && node dist/app/createVesting.js",
    "vesting:claim": "npm run build:light && node dist/app/claimVesting.js",
    "vesting:emulator": "npm run build:light && node dist/app/emulator_integration/vesting_emulator.js",
    "test:emulator": "npm run build:light && node dist/test/testE2E.js --emulator",
    "test:blockfrost": "npm run build:light && node dist/test/testE2E.js",
    "test:emulator:returnFaucet": "npm run build:light && node dist/test/testE2E.js --emulator --returnFaucet",
    "test:blockfrost:returnFaucet": "npm run build:light && node dist/test/testE2E.js --returnFaucet"
  },
  "dependencies": {
    "@harmoniclabs/blockfrost-pluts": "^0.1.16",
    "@harmoniclabs/buildooor": "^0.1.9",
    "@harmoniclabs/plu-ts": "^0.9.0",
    "@harmoniclabs/pluts-emulator": "^0.0.1-dev5",
    "blakejs": "^1.2.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "tsc-alias": "^1.7.1",
    "typescript": "^4.8.4"
  }
}
